<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TeamActivity">

    <TextView
        android:id="@+id/twTim1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/btnUI"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/twTim2"
        style="@android:style/Widget.DeviceDefault.TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/btnUI"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ic_acc"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/tel1"
        android:layout_width="75dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_tel"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/btnUI"
        app:layout_constraintEnd_toEndOf="@+id/twUSR1"
        app:layout_constraintStart_toStartOf="@+id/twUSR1"
        app:layout_constraintTop_toBottomOf="@+id/twUSR1"
        app:layout_constraintVertical_bias="0.0"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/twUSR1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/btnUI"
        app:layout_constraintEnd_toEndOf="@+id/twTim1"
        app:layout_constraintStart_toStartOf="@+id/twTim1"
        app:layout_constraintTop_toBottomOf="@+id/twTim1"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ic_acc"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/tel2"
        android:layout_width="75dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_tel"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/btnUI"
        app:layout_constraintEnd_toEndOf="@+id/twUSR"
        app:layout_constraintStart_toStartOf="@+id/twUSR"
        app:layout_constraintTop_toBottomOf="@+id/twUSR"
        app:layout_constraintVertical_bias="0.0"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/twUSR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/btnUI"
        app:layout_constraintEnd_toEndOf="@+id/twTim2"
        app:layout_constraintStart_toStartOf="@+id/twTim2"
        app:layout_constraintTop_toBottomOf="@+id/twTim2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnUI"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/dugmetara"
        android:ems="10"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

</androidx.constraintlayout.widget.ConstraintLayout>
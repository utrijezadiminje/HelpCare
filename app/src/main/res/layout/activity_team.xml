<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TeamActivity">

    <TextView
        android:id="@+id/twTim1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tel1"
        app:layout_constraintEnd_toEndOf="@+id/tel1"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/tel1"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/twTim2"
        style="@android:style/Widget.DeviceDefault.TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="172dp"
        android:layout_marginBottom="43dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tel2"
        app:layout_constraintEnd_toEndOf="@+id/tel2"
        app:layout_constraintHorizontal_bias="0.588"
        app:layout_constraintStart_toStartOf="@+id/tel2"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.855" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="99dp"
        android:layout_height="94dp"
        android:layout_marginBottom="82dp"
        android:src="@drawable/ic_acc"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/tel1"
        app:layout_constraintEnd_toEndOf="@+id/tel1"
        app:layout_constraintStart_toStartOf="@+id/tel1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="99dp"
        android:layout_height="94dp"
        android:src="@drawable/ic_acc"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/tel2"
        app:layout_constraintEnd_toEndOf="@+id/tel2"
        app:layout_constraintStart_toStartOf="@+id/tel2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/tel1"
        android:layout_width="74dp"
        android:layout_height="57dp"
        android:layout_marginEnd="304dp"
        android:src="@drawable/ic_tel"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.536"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/tel2"
        android:layout_width="74dp"
        android:layout_height="57dp"
        android:layout_marginEnd="44dp"
        android:src="@drawable/ic_tel"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btnUI"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.537"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/twUSR1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/tel1"
        app:layout_constraintEnd_toEndOf="@+id/tel1"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/tel1"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.253" />

    <TextView
        android:id="@+id/twUSR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/tel2"
        app:layout_constraintEnd_toEndOf="@+id/tel2"
        app:layout_constraintHorizontal_bias="0.588"
        app:layout_constraintStart_toStartOf="@+id/tel2"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.253" />

    <Button
        android:id="@+id/btnUI"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/dugmetara"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.912" />

</androidx.constraintlayout.widget.ConstraintLayout>